{
  "name": "membership-management-server",
  "version": "1.0.0",
  "main": "index.js",
  "author": "David Moreira",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "node dist/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpileOnly --ignore-watch --no-notify src/server.ts",
    "pretest": "cross-env NODE_ENV=test npx knex --knexfile knexfile.ts migrate:rollback --all && cross-env NODE_ENV=test npx knex --knexfile knexfile.ts migrate:latest && cross-env NODE_ENV=test npx knex --knexfile knexfile.ts seed:run",
    "test": "cross-env NODE_ENV=test jest --testTimeout=5000",
    "knex:migrate": "npx knex --knexfile knexfile.ts migrate:latest",
    "knex:seed": "npx knex --knexfile knexfile.ts seed:run",
    "knex:rollback": "npx knex --knexfile knexfile.ts migrate:rollback --all"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "celebrate": "^12.1.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.4.0",
    "pg": "^8.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-typescript": "^7.10.1",
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/hapi__joi": "^17.1.3",
    "@types/jest": "^26.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.14",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.3.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.0.1",
    "sqlite3": "^4.2.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-node-dev": "^1.0.0-pre.49",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.5"
  }
}
